        "userId": {
          "type": "integer",
          "format": "int64"
        },
        "keepUntilDate": {
          "type": "string",
          "format": "date-time"
        },
        "weight": {
          "type": "integer",
          "format": "int32"
        },
        "price": {
          "type": "integer",
          "format": "int32"
        },
        "packageType": {
          "$ref": "#/definitions/v1PackageType"
        },
        "needWrapping": {
          "type": "boolean"
        }
      },
      "required": [
        "orderId",
        "userId",
        "keepUntilDate",
        "weight",
        "price",
        "packageType",
        "needWrapping"
