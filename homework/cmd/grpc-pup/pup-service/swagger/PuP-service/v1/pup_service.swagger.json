            "$ref": "#/definitions/v1ReturnV1"
          }
        }
      }
    },
    "v1OrderFromListV1": {
      "type": "object",
      "properties": {
        "orderId": {
          "type": "integer",
          "format": "int64"
        },
        "userId": {
          "type": "integer",
          "format": "int64"
        },
        "state": {
          "type": "string"
        },
        "keepUntilDate": {
          "type": "string",
          "format": "date-time"
        },
        "price": {
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "v1PackageType": {
      "type": "string",
      "enum": [
        "PACKAGE_UNKNOWN",
        "BOX",
